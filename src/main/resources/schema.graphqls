type Query {
    myMaze(mazeId: ID!): Maze
}

type Mutation {
    createAMaze(mazeRunner: String!): ID!
    saySomething(message: String!): String
}

type Subscription {
    tellMeSeconds: Int!
    tellMeWhatOthersSay: String
}

type Maze {
    yourPosition: Position!
}

type Position {
    x: Int!
    y: Int!
}
